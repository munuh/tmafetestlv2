generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model Owner {
  id         Int     @id
  owner_name String? @db.VarChar(225)
}

model Product {
  product_id    Int     @id
  product_name  String? @db.VarChar(85)
  product_brand String? @db.VarChar(45)
  created_date  String? @db.VarChar(45)
}

model ProductOwner {
  products_id Int     @id
  owners_id   String? @db.VarChar(45) 
  @@map("products_owners")
}

